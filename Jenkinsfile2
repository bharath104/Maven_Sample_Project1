pipeline {
        agent any
            stages {
                stage ('validate') {   
                    steps {
                        bat 'mvn validate'
                    }
                }   
                stage ('compile') {
                        steps {
                            bat 'mvn compile'
                        }
                } 
                stage ('package') {
                    steps {
                        bat 'mvn package'
                    }
                } 
                stage ('install') {
                    steps {
                        bat 'java -jar target/java-project2-1.0-SNAPSHOT.jar'
                    }
                } 
                        stage('verify build output')
                        {
                        steps {
                        bat 'dir target'
                        }
                        }
            }
}
